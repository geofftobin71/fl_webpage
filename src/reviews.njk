---
title: Reviews
layout: layouts/base.njk
---

<main>
<section class="wrapper">
  <h1>{{ title }}</h1>
  <ul class="flow">
    {% for item in reviews | shuffle -%}
      <li>
        <p>{{ item.review }}</p>
        <p style="text-align:right">{{ item.name }}</p>
      </li>
    {% endfor -%}
  </ul>
</section>
</main>
