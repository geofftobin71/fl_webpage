---
layout: layouts/base.njk
---

<h1>Foriade</h1>

<p>Hello World!</p>

<section class="hero is-info">
<div class="hero-body">
{{ collections.instagram }}
</div>
</section>

{% for insta in collections.instagram %}
  <img src="{{ insta.media_url  }}">
  <p>{{ insta.caption }}</p>
{% endfor %}

<ul>
  {% for post in collections.posts | reverse %}
  <li>
    <a href="{{ post.url }}">{{ post.data.title }}</a>
  </li>
  {% endfor %}
</ul>
